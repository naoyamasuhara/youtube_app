<h1>動画一覧</h1>

<%= link_to '動画を投稿する', new_video_path, class: "btn btn-primary" %>

<table>
  <tr>
    <th>タイトル</th>
    <th>投稿日時</th>
    <th colspan= "2"></th>
  </tr>

  <% @videos.each do |video| %>
    <tr>
      <td><%= link_to video.title, video_path(video) %></td>
      <td><%= video.created_at.strftime('%Y-%m-%d %X') %></td>
    </tr>
  <% end %>
</table>