<h1>動画を編集する</h1>

<%= form_for(@video) do |f| %>
  <p>
    <%= f.label :タイトル %><br>
    <%= f.text_field :title, class: 'form-control' %>
  </p>

  <p>
    <%= f.label :コメント %><br>
    <%= f.text_area :content, class: 'form-control' %><br>
  </p>

  <p>
    <%= f.label :動画のURL %><br>
    <%= f.text_field :youtube_uuid, class: 'form-control' %><br>
  </p>
<% end %>

<%= link_to '一覧',videos_path %>